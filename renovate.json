{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "timezone": "Asia/Tokyo",
  "labels": [
    "renovate"
  ],
  "enabledManagers": ["dockerfile", "npm"],
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "enabled": true
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  }
}
